{
  "name": "discogs-data-tools",
  "version": "0.3.0",
  "description": "Tools to download and process Discogs data dumps",
  "author": "Floris Bernard <floris.bernard@gmail.com> (https://github.com/flut1)",
  "repository": {
    "type": "git",
    "url": "git@github.com:flut1/discogs-data-tools.git"
  },
  "bin": "./cli/index.js",
  "keywords": [
    "discogs",
    "data",
    "parser",
    "utils"
  ],
  "license": "MIT",
  "scripts": {
    "compile": "tsc --project .",
    "dev": "npm run compile && node cli/index",
    "dev-latest": "npm run compile && node cli/index stream --dump-version 20200703 --progress",
    "prettify": "prettier --write \"src/*.js\"",
    "lint": "eslint .",
    "help-to-readme": "node scripts/help-to-readme.js",
    "docs": "jsdoc2md src/*.js > api.md"
  },
  "main": "src/index.js",
  "module": "main.js",
  "engines": {
    "node": ">=12.18.1"
  },
  "dependencies": {
    "@elastic/elasticsearch": "^7.8.0",
    "esm": "^3.2.25",
    "fs-extra": "^9.0.1",
    "idx": "^2.5.6",
    "ink": "github:flut1/ink#typescript-box-ref-module",
    "lodash-es": "^4.17.15",
    "node-expat": "^2.3.18",
    "node-fetch": "^2.6.0",
    "react": "^16.13.1",
    "xml2js": "^0.4.23",
    "yargs": "^15.3.1"
  },
  "devDependencies": {
    "@types/lodash-es": "^4.17.3",
    "@types/node": "^14.0.14",
    "@types/node-expat": "^2.3.1",
    "@types/node-fetch": "^2.5.7",
    "@types/react": "^16.9.42",
    "@types/xml2js": "^0.4.5",
    "eslint": "^7.3.1",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-prettier": "^3.1.4",
    "jsdoc-to-markdown": "^6.0.1",
    "prettier": "^2.0.5",
    "typescript": "^3.9.6"
  }
}
